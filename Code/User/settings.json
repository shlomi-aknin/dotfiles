{
    "editor.lineNumbers": "relative",
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
    "workbench.startupEditor": "newUntitledFile",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.minimap.enabled": false,
    "workbench.activityBar.visible": false,
    "window.menuBarVisibility": "toggle",
    "editor.cursorSurroundingLines": 999,
    "vim.easymotion": true,
    "workbench.colorCustomizations": {
      "editorWarning.foreground": "#00000000",
      "editorInfo.foreground":    "#00000000",
      "gitlens.trailingLineForegroundColor":"#FF79C6"
    },
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["g", "o"],
        "commands": ["workbench.action.joinAllGroups"]
      },
      {
        "before": ["g", "c"],
        "commands": ["editor.action.triggerSuggest"],
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
      },
      {
        "before": ["g", "p", "v"],
        "commands": ["workbench.action.splitEditorDown", "editor.action.goToImplementation", "workbench.action.previousEditor", "workbench.action.revertAndCloseActiveEditor"],
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
      },
      {
        "before": ["g", "p", "d"],
        "commands": ["editor.action.revealDefinitionAside"],
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
      },
      {
        "before": ["g", "k"],
        "commands": ["editor.action.peekTypeDefinition"]
      },
      {
        "before": ["c"],
        "after": ["\"", "_", "c"]
      },
      {
        "before": ["C"],
        "after": ["\"", "_", "C"]
      },
      {
        "before": ["d"],
        "after": ["\"", "_", "d"]
      },
      {
        "before": ["s"],
        "after": ["\"", "_", "s"]
      },
      {
        "before": ["S"],
        "after": ["\"", "_", "S"]
      },
      {
        "before": ["x"],
        "after": ["\"", "_", "x"]
      },
      {
        "before": ["X"],
        "after": ["\"", "_", "X"]
      },
      {
        "before": ["m"],
        "after": ["x"]
      },
      {
        "before": ["M"],
        "after": ["d"]
      },
      {
        "before": ["v", "i", "}"],
        "after": ["$", "F", "{", "l", "v", "t", "}"]
      },
      {
        "before": ["v", "i", "]"],
        "after": ["$", "F", "[", "l", "v", "t", "]"]
      },
      {
        "before": ["v", "i", ")"],
        "after": ["$", "F", "(", "l", "v", "t", ")"]
      },
      {
        "before": ["c", "i", "}"],
        "after": ["$", "F", "{", "l", "v", "t", "}", "x", "i"]
      },
      {
        "before": ["d", "i", "}"],
        "after": ["$", "F", "{", "l", "v", "t", "}", "x"]
      },
      {
        "before": ["c", "i", "]"],
        "after": ["$", "F", "[", "l", "v", "t", "]", "x", "i"]
      },
      {
        "before": ["d", "i", "]"],
        "after": ["$", "F", "[", "l", "v", "t", "]", "x"]
      },
      {
        "before": ["c", "i", ")"],
        "after": ["$", "F", "(", "l", "v", "t", ")", "x", "i"]
      },
      {
        "before": ["d", "i", ")"],
        "after": ["$", "F", "(", "l", "v", "t", ")", "x"]
      },
      {
        "before": ["d", ")"],
        "after": ["$", "F", "(", "x", "f", ")", "x"]
      },
      {
        "before": ["d", "("],
        "after": ["$", "F", "(", "2", "x", "f", ")", "h", "2", "x"]
      },
      {
        "before": ["d", "}"],
        "after": ["$", "F", "{", "x", "f", "}", "x"]
      },
      {
        "before": ["d", "{"],
        "after": ["$", "F", "{", "2", "x", "f", "}", "h", "2", "x"]
      },
      {
        "before": ["d", "]"],
        "after": ["$", "F", "[", "x", "f", "]", "x"]
      },
      {
        "before": ["d", "["],
        "after": ["$", "F", "[", "2", "x", "f", "]", "h", "2", "x"]
      },
      {
        "before": ["g", "b"],
        "commands": ["gitlens.toggleLineBlame"]
      },
      {
        "before": ["g", "e"],
        "commands": ["find-it-faster.findFiles"]
      },
      {
        "before": ["g", "s"],
        "commands": ["find-it-faster.findWithinFiles"]
      },
      {
        "before": ["g", "d"],
        "commands": ["editor.action.revealDefinition"],
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
      },
      {
        "before": ["g", "i"],
        "commands": ["editor.action.goToImplementation"],
        "when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
      },
      {
        "before": ["g", "r"],
        "commands": ["references-view.findReferences"],
        "when": "editorHasReferenceProvider"
      },
      {
        "before": ["g", "p", "r"],
        "commands": ["references-view.findReferences"],
        "when": "editorHasReferenceProvider"
      },
      {
        "before": ["g", "j", "d"],
        "commands": ["extension.debugJest"],
        "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode == 'Normal'"
      },
      {
        "before": ["g", "j", "f"],
        "commands": ["extension.runJestFile"],
        "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode == 'Normal'"
      },
      {
        "before": ["s", "i", "\""],
        "after": ["v", "i", "\"", "p"],
      },
      {
        "before": ["s", "a", "\""],
        "after": ["v", "a", "\"", "p"],
      },
      {
        "before": ["s", "i", "("],
        "after": ["v", "i", "(", "p"],
      },
      {
        "before": ["s", "a", "("],
        "after": ["v", "a", "(", "p"],
      },
      {
        "before": ["s", "a", ")"],
        "after": ["v", "a", ")", "p"],
      },
      {
        "before": ["s", "i", ")"],
        "after": ["v", "i", ")", "p"],
      },
      {
        "before": ["s", "i", "{"],
        "after": ["v", "i", "{", "p"],
      },
      {
        "before": ["s", "a", "{"],
        "after": ["v", "a", "{", "p"],
      },
      {
        "before": ["s", "i", "}"],
        "after": ["v", "i", "}", "p"],
      },
      {
        "before": ["s", "a", "}"],
        "after": ["v", "a", "}", "p"],
      },
      {
        "before": ["s", "i", "["],
        "after": ["v", "i", "[", "p"],
      },
      {
        "before": ["s", "a", "["],
        "after": ["v", "a", "[", "p"],
      },
      {
        "before": ["s", "i", "]"],
        "after": ["v", "i", "]", "p"],
      },
      {
        "before": ["s", "a", "]"],
        "after": ["v", "a", "]", "p"],
      },
      {
        "before": ["s", "i", "w"],
        "after": ["v", "i", "w", "p"],
      },
      {
        "before": ["s", "a", "w"],
        "after": ["v", "a", "w", "p"],
      },
      {
        "before": ["s", "i", "'"],
        "after": ["v", "i", "'", "p"],
      },
      {
        "before": ["s", "a", "\""],
        "after": ["v", "a", "\"", "p"],
      },
      {
        "before": ["g", ")"],
        "after": ["$", "F", "(", "a"]
      },
      {
        "before": ["g", "}"],
        "after": ["$", "F", "{", "a"]
      },
      {
        "before": ["g", "]"],
        "after": ["$", "F", "[", "a"]
      },
      {
        "before": ["g", "("],
        "after": ["$", "F", "(", "a", "<cr>"]
      },
      {
        "before": ["g", "{"],
        "after": ["$", "F", "{", "a", "<cr>"]
      },
      {
        "before": ["g", "["],
        "after": ["$", "F", "[", "a", "<cr>"]
      },
      {
        "before": ["g", "m"],
        "after": ["o", "<esc>", "O", "<esc>"]
      },
      {
        "before": ["g", "v"],
        "after": ["V", "$", "%"]
      },
      {
        "before": ["<space>", "j"],
        "commands": ["editor.action.insertLineAfter"],
        "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode == 'Normal'"
      },
      {
        "before": ["<space>", "k"],
        "commands": ["editor.action.insertLineBefore"],
        "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode != 'Normal'"
      },
      {
        "before": ["<space>", "q", "q"],
        "commands": ["workbench.action.revertAndCloseActiveEditor"],
        "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode == 'Normal'"
      },
      {
        "before": ["<space>", "<space>"],
        "after": ["\\", "\\", "\\", "b", "d", "w"]
      },
      {
        "before": ["d", "w"],
        "after": ["d", "i", "w"]
      },
      {
        "before": ["c", "w"],
        "after": ["c", "i", "w"]
      },
      {
        "before": ["X"],
        "after": ["g", "g", "d", "G"]
      },
      {
        "before": ["K"],
        "commands": ["editor.action.showDefinitionPreviewHover"]
      },
      {
        "before": ["%"],
        "commands": ["extension.matchitJumpItems"]
      },
      {
        "before": ["<space>", "b"],
        "commands": ["workbench.action.showAllEditors"]
      },
      {
        "before": ["<space>", "p"],
        "commands": ["workbench.action.gotoSymbol"]
      },
      {
        "before": ["<space>", "r"],
        "commands": ["editor.action.rename"],
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
      },
      {
        "before": ["<space>", "s"],
        "commands": ["workbench.action.files.save"],
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
      },
      {
        "before": ["<space>", "1"],
        "commands": ["workbench.action.openSettingsJson"]
      },
      {
        "before": ["<space>", "x"],
        "commands": ["workbench.action.closeAllEditors"]
      },
      {
        "before": ["<space>", "S"],
        "commands": ["workbench.action.files.saveAll"]
      },
      {
        "before": ["<space>", "h"],
        "commands": [":nohl"]
      },
      {
        "before": ["<S-l>"],
        "after": ["g", "_"]
      },
      {
        "before": ["<S-h>"],
        "after": ["^"]
      },
      {
        "before": ["<space>", "t"],
        "commands": [
          "workbench.action.terminal.toggleTerminal",
          "workbench.action.terminal.focus"
        ]
      },
      {
        "before": ["<space>", "g"],
        "commands": [
          ":!lazygitcode"
        ]
      },
      {
        "before": ["<space>", "z"],
        "commands": [
          ":!vifmcode"
        ]
      },
      {
        "before": ["<space>", "u"],
        "commands": [
          ":!alacritty -t 'updatelastproject' --class 'updatelastproject' -e updatelastproject"
        ]
      },
      {
        "before": ["<space>", "f"],
        "commands": [
          ":!alacritty -t 'fzfcode' --class 'fzfcode' -e fzfsearchforcode"
          // "workbench.action.quickOpen"
        ]
      },
      {
        "before": ["<space>", "x"],
        "commands": [
          "workbench.action.closeAllEditors",
          "workbench.action.files.newUntitledFile"
        ]
      },
      {
        "before": ["<space>", "e"],
        "commands": [
          "workbench.action.toggleSidebarVisibility",
          "workbench.view.explorer",
          "revealInExplorer"
        ],
      },
      {
        "before": ["<space>", "/"],
        "commands": ["editor.action.commentLine"]
      },
      {
        "before": ["g", "l"],
        "after": ["a", "<space>", "<esc>"]
      },
      {
        "before": ["g", "h"],
        "after": ["i", "<space>", "<esc>"]
      },
      {
        "before": ["<space>", "c", "a"],
        "commands": ["editor.action.sourceAction"],
        "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)quickfix\\b/"
      },
      {
        "before": ["<space>", "d"],
        "commands": ["editor.action.peekDefinition"],
        "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
      },
      {
        "before": ["s", "s"],
        "after": ["V", "p"]
      },
      {
        "before": ["s", "i", "w"],
        "after": ["v", "i", "w", "p"]
      },
      {
        "before": ["s", "i", "("],
        "after": ["v", "i", "(", "p"]
      },
      {
        "before": ["s", "i", "{"],
        "after": ["v", "i", "{", "p"]
      },
      {
        "before": ["s", "i", "["],
        "after": ["v", "i", "[", "p"]
      },
      {
        "before": ["s", "i", "p"],
        "after": ["v", "i", "p", "p"]
      },
      {
        "before": ["s", "i", "'"],
        "after": ["v", "i", "'", "p"]
      },
      {
        "before": ["s", "a", "'"],
        "after": ["v", "a", "'", "p"]
      },
      {
        "before": ["s", "i", "`"],
        "after": ["v", "i", "`", "p"]
      },
      {
        "before": ["s", "a", "`"],
        "after": ["v", "a", "`", "p"]
      },
      {
        "before": ["s", "a", "("],
        "after": ["v", "a", "(", "p"]
      },
      {
        "before": ["s", "a", "{"],
        "after": ["v", "a", "{", "p"]
      },
      {
        "before": ["s", "a", "["],
        "after": ["v", "a", "[", "p"]
      },
      {
        "before": ["s", "a", "p"],
        "after": ["v", "a", "p", "p"]
      },
      {
        "before": ["s", "E"],
        "after": ["v", "E", "p"]
      },
      {
        "before": ["s", "f", ">"],
        "after": ["v", "f", ">", "p"]
      },
      {
        "before": ["s", "f", ")"],
        "after": ["v", "f", ")", "p"]
      },
      {
        "before": ["s", "f", "}"],
        "after": ["v", "f", "}", "p"]
      },
      {
        "before": ["s", "f", "]"],
        "after": ["v", "f", "]", "p"]
      },
      {
        "before": ["s", "f", ","],
        "after": ["v", "f", ",", "p"]
      },
      {
        "before": ["s", "t", ","],
        "after": ["v", "t", ",", "p"]
      },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
      {
        "before": [">"],
        "commands": ["editor.action.indentLines"]
        // "after": [">", "g", "v"]
      },
      {
        "before": ["<"],
        "commands": ["editor.action.reindentselectedlines"]
        // "after": ["<", "g", "v"]
      },
      {
        "before": ["p"],
        "after": ["p", "g", "v", "y"]
      },
      {
        "before": ["c"],
        "after": ["\"", "_", "c"]
      },
      {
        "before": ["C"],
        "after": ["\"", "_", "C"]
      },
      {
        "before": ["d"],
        "after": ["\"", "_", "d"]
      },
      {
        "before": ["s"],
        "after": ["\"", "_", "s"]
      },
      {
        "before": ["S"],
        "after": ["\"", "_", "S"]
      },
      {
        "before": ["x"],
        "after": ["\"", "_", "x"]
      },
      {
        "before": ["m"],
        "after": ["x"]
      },
      {
        "before": ["M"],
        "after": ["d"]
      }
    ],
    "vim.visualModeKeyBindings": [
      {
        "before": ["<space>", "/"],
        "commands": ["editor.action.commentLine"]
      }
  ],
  "files.autoSave": "onFocusChange",
  "jestrunner.runOptions": [
    "--coverage=false"
  ],
  "editor.fontWeight": "600",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
    "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "editor.fontLigatures": true,
  "vim.useSystemClipboard": true,
  "window.zoomLevel": 1,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Dracula At Night"
}