{
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "'SauceCodePro Nerd Font','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.openEditors.visible": 30,
  "find-it-faster.general.showMaximizedTerminal": true,
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.cursorStylePerMode.visual": "underline",
  "vim.cursorStylePerMode.visualblock": "underline",
  "vim.cursorStylePerMode.visualline": "underline",
  "vim.easymotionIncSearchForegroundColor": "#f7wf38",
  "vim.easymotionMarkerForegroundColorOneChar": "#ffff00",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#34a1eb",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#ffffff",
  "vim.highlightedyank.color": "#34a1eb",
  "vim.highlightedyank.duration": 1200,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "#000000",
  "vim.hlsearch": true,
  "vim.smartRelativeLine": true,
  "vim.searchHighlightColor": "#34a1eb90",
  "vim.useSystemClipboard": true,
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 2.5,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Dracula At Night",
  "workbench.list.smoothScrolling": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "workbench.colorCustomizations": {
    "editorGroupHeader.tabsBackground": "#000000",
    "editorLineNumber.activeForeground": "#ffff00",
    "editor.lineHighlightBackground": "#235789",
    "editorBracketMatch.background": "#2176FF",
    "tab.activeBackground": "#34a1eb",
    "tab.activeForeground": "#000000",
    "tab.inactiveForeground": "#ffffff"
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [ "m" ],
      "after": [ "o", "<esc>" ]
    },
    {
      "before": [ "M" ],
      "after": [ "O", "<esc>" ]
    },
    {
      "before": [ "s" ],
      "after": [ "\\", "\\", "\\", "j" ]
    },
    {
      "before": [ "d", "w" ],
      "after": [ "d", "i", "w" ]
    },
    {
      "before": [ "c", "w" ],
      "after": [ "c", "i", "w" ]
    },
    {
      "before": [ "X" ],
      "after": [ "g", "g", "d", "G" ]
    },
    {
      "before": [ "K" ],
      "commands": [ "editor.action.showDefinitionPreviewHover" ]
    },
    {
      "before": [ "%" ],
      "commands": [ "extension.matchitJumpItems" ]
    },
    {
      "before": [ "<space>", "b" ],
      "commands": [ "workbench.action.showAllEditors" ]
    },
    {
      "before": [ "<space>", "c" ],
      "commands": [ "workbench.action.toggleStatusbarVisibility" ]
    },
    {
      "before": [ "<space>", "p" ],
      "commands": [ "workbench.action.gotoSymbol" ]
    },
    {
      "before": [ "<space>", "r" ],
      "commands": [ ":e!" ]
    },
    {
      "before": [ "<space>", "1" ],
      "commands": [ "workbench.action.openSettingsJson" ]
    },
    {
      "before": [ "<space>", "s" ],
      "commands": [ "workbench.action.files.save" ]
    },
    {
      "before": [ "<space>", "x" ],
      "commands": [ "workbench.action.closeAllEditors" ]
    },
    {
      "before": [ "<space>", "S" ],
      "commands": [ "workbench.action.files.saveAll" ]
    },
    {
      "before": [ "<space>", "h" ],
      "commands": [ ":nohl" ]
    },
    {
      "before": [ "<S-l>" ],
      "after": [ "a", "<space>", "<esc>" ]
    },
    {
      "before": [ "<S-h>" ],
      "after": [ "i", "<space>", "<esc>" ]
    },
    {
      "before": [ "<space>", "d" ],
      "commands": [ "workbench.action.showCommands" ]
    },
    {
      "before": [ "<space>", "t" ],
      "commands": [ "workbench.action.terminal.toggleTerminal", "workbench.action.terminal.focus" ]
    },
    {
      "before": [ "<space>", "e" ],
      "commands": [ "workbench.action.toggleSidebarVisibility", "revealInExplorer", "workbench.files.action.focusFilesExplorer" ]
    },
    {
      "before": [ "<space>", "g" ],
      "commands": [ ":!alacritty -e lazygit -p $(cat /tmp/lastdir) &" ]
    },
    {
      "before": [ "<space>", "f" ],
      "commands": [ ":!alacritty -e fzfsearchforcode &" ]
    },
    {
      "before": [ "<space>", "o" ],
      "commands": [ "workbench.action.quickOpen" ]
    },
    {
      "before": [ "<space>", "x" ],
      "commands": [ "workbench.action.closeAllEditors", "workbench.action.files.newUntitledFile" ]
    },
    {
      "before": [ "<space>", "z" ],
      "commands": [ ":!alacritty -e vifm +only $(cat /tmp/lastdir) &" ]
    },
    {
      "before": [ "<space>", "/" ],
      "commands": [ "editor.action.commentLine" ]
    },
    {
      "before": [ "<C-o>" ],
      "commands": [ "workbench.action.files.openFolder" ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [ "<space>", "/" ],
      "commands": [ "editor.action.commentLine" ]
    },
  ],
  "vim.insertModeKeyBindings": [],
  "tabnine.experimentalAutoImports": true,
  "php-cs-fixer.lastDownload": 1633618399698,
  "sync.autoDownload": true,
  "sync.forceUpload": true,
  "sync.autoUpload": true,
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "vim.easymotion": true,
  "sync.gist": "2e47474f8b6b139d6d1e2a491314795e",
  "settingsSync.ignoredExtensions": [
    "shan.code-settings-sync"
  ],
  "editor.tokenColorCustomizations": {
    "[Dracula At Night]": {
      "textMateRules": [
        {
          "scope": "comment",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#C69650"
          }
        },
        {
          "settings": {
            "foreground": "#ff79c6"
          },
          "scope": "support.class.component.svelte"
        }
      ]
    }
  },
  "extensions.ignoreRecommendations": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "openMultipleFiles.exclude": "node_modules/**",
  "workbench.editor.enablePreview": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.banner": "never",
  "explorer.confirmDelete": false,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.lineNumbers": "relative",
  "svelte.enable-ts-plugin": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "json.format.enable": false,
  "files.autoSave": "onFocusChange",
  "editor.cursorSurroundingLines": 999,
  "workbench.statusBar.visible": false
}