{
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "'SauceCodePro Nerd Font','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.openEditors.visible": 30,
  "find-it-faster.general.showMaximizedTerminal": true,
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.cursorStylePerMode.visual": "underline",
  "vim.cursorStylePerMode.visualblock": "underline",
  "vim.cursorStylePerMode.visualline": "underline",
  "vim.easymotionIncSearchForegroundColor": "#ffff00",
  "vim.easymotionKeys": "hjklasdf",
  "vim.easymotionMarkerForegroundColorOneChar": "#ffff00",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#50fa7b",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#ff79c6",
  "vim.highlightedyank.color": "#50fa7b",
  "vim.highlightedyank.duration": 1200,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "#000000",
  "vim.hlsearch": true,
  "vim.searchHighlightColor": "#50fa7b",
  "vim.smartRelativeLine": true,
  "vim.useSystemClipboard": true,
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 2.5,
  "workbench.activityBar.visible": false,
  "workbench.list.smoothScrolling": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "workbench.colorCustomizations": {
    "badge.background": "#00BBF9",
    "badge.foreground": "#000000",
    "editor.lineHighlightBackground": "#44475a",
    "editorBracketMatch.background": "#2176FF",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorLineNumber.activeForeground": "#ffff00",
    "editorLineNumber.foreground": "#ffffff",
    "editorSuggestWidget.focusHighlightForeground": "#2C0972",
    "editorSuggestWidget.foreground": "#ffffff",
    "editorSuggestWidget.highlightForeground": "#F15BB5",
    "editorSuggestWidget.selectedBackground": "#00BBF9",
    "editorSuggestWidget.selectedForeground": "#000000",
    "quickInputList.focusBackground": "#00BBF9",
    "quickInputList.focusForeground": "#000000",
    "statusBar.background": "#00BBF9",
    "statusBar.debuggingBackground": "#00BBF9",
    "statusBar.debuggingForeground": "#000000",
    "statusBar.foreground": "#000000",
    "statusBar.noFolderBackground": "#00BBF9",
    "tab.activeBackground": "#34a1eb",
    "tab.activeForeground": "#000000",
    "tab.inactiveForeground": "#ffffff"
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.easymotionmode": ["#ffff00", "#000000"],
  "vim.statusBarColors.insert": ["#44AF69", "#000000"],
  "vim.statusBarColors.normal": ["#00BBF9", "#000000"],
  "vim.statusBarColors.replace": ["#F15BB5", "#000000"],
  "vim.statusBarColors.searchinprogressmode": ["#007ACC", "#ffffff"],
  "vim.statusBarColors.visual": ["#FCAB10", "#000000"],
  "vim.statusBarColors.visualblock": ["#FCAB10", "#000000"],
  "vim.statusBarColors.visualline": ["#FCAB10", "#000000"],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["m"],
      "after": ["o", "<esc>"]
    },
    {
      "before": ["M"],
      "after": ["O", "<esc>"]
    },
    {
      "before": ["s"],
      "after": ["\\", "\\", "\\", "b", "d", "w"]
    },
    {
      "before": ["d", "w"],
      "after": ["d", "i", "w"]
    },
    {
      "before": ["c", "w"],
      "after": ["c", "i", "w"]
    },
    {
      "before": ["X"],
      "after": ["g", "g", "d", "G"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["%"],
      "commands": ["extension.matchitJumpItems"]
    },
    {
      "before": ["<space>", "b"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<space>", "p"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<space>", "r"],
      "commands": ["editor.action.rename"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<space>", "s"],
      "commands": ["workbench.action.files.save"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<space>", "1"],
      "commands": ["workbench.action.openSettingsJson"]
    },
    {
      "before": ["<space>", "x"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<space>", "S"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<space>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<S-l>"],
      "after": ["a", "<space>", "<esc>"]
    },
    {
      "before": ["<S-h>"],
      "after": ["i", "<space>", "<esc>"]
    },
    {
      "before": ["<space>", "t"],
      "commands": [
        "workbench.action.terminal.toggleTerminal",
        "workbench.action.terminal.focus"
      ]
    },
    {
      "before": ["<space>", "e"],
      "commands": [
        "workbench.action.toggleSidebarVisibility",
        "revealInExplorer",
        "workbench.files.action.focusFilesExplorer"
      ]
    },
    {
      "before": ["<space>", "g"],
      "commands": [
        ":!alacritty -t 'LAZYGITCODE' --class 'lazygitcode' -e lazygit -p $(cat /tmp/lastdir) &"
      ]
    },
    {
      "before": ["<space>", "f"],
      "commands": [
        ":!alacritty -t 'FZFCODE' --class 'fzfcode' -e fzfsearchforcode &"
      ]
    },
    {
      "before": ["<space>", "o"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<space>", "x"],
      "commands": [
        "workbench.action.closeAllEditors",
        "workbench.action.files.newUntitledFile"
      ]
    },
    {
      "before": ["<space>", "z"],
      "commands": [
        ":!alacritty -t 'VIFMCODE' --class 'vifmcode' -e vifm +only $(cat /tmp/lastdir) &"
      ]
    },
    {
      "before": ["<space>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<C-o>"],
      "commands": ["workbench.action.files.openFolder"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    },
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<space>", "/"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.insertModeKeyBindings": [],
  "tabnine.experimentalAutoImports": true,
  "php-cs-fixer.lastDownload": 1633618399698,
  "sync.autoDownload": true,
  "sync.forceUpload": true,
  "sync.autoUpload": true,
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "vim.easymotion": true,
  "sync.gist": "2e47474f8b6b139d6d1e2a491314795e",
  "settingsSync.ignoredExtensions": ["shan.code-settings-sync"],
  "editor.tokenColorCustomizations": {
    "[Dracula At Night]": {
      "textMateRules": [
        {
          "scope": "comment",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#C69650"
          }
        },
        {
          "settings": {
            "foreground": "#ff79c6"
          },
          "scope": "support.class.component.svelte"
        }
      ]
    }
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "explorer.confirmDelete": false,
  "extensions.ignoreRecommendations": true,
  "openMultipleFiles.exclude": "node_modules/**",
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "workbench.editor.enablePreview": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.cursorSurroundingLines": 999,
  "files.autoSave": "onFocusChange",
  "json.format.enable": false,
  "vim.searchHighlightTextColor": "#000000",
  "workbench.colorTheme": "Dracula At Night",
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "material-icon-theme"
}
