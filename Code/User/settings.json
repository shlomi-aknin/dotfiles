      "after": ["$", "F", "[", "l", "v", "t", "]", "x"]
    },
    {
      "before": ["c", "i", ")"],
      "after": ["$", "F", "(", "l", "v", "t", ")", "x", "i"]
    },
    {
      "before": ["d", "i", ")"],
      "after": ["$", "F", "(", "l", "v", "t", ")", "x"]
    },
    {
      "before": ["d", ")"],
      "after": ["$", "F", "(", "x", "f", ")", "x"]
    },
    {
      "before": ["d", "("],
      "after": ["$", "F", "(", "2", "x", "f", ")", "h", "2", "x"]
    },
    {
      "before": ["d", "}"],
      "after": ["$", "F", "{", "x", "f", "}", "x"]
    },
    {
      "before": ["d", "{"],
      "after": ["$", "F", "{", "2", "x", "f", "}", "h", "2", "x"]
    },
    {
      "before": ["d", "]"],
      "after": ["$", "F", "[", "x", "f", "]", "x"]
    },
    {
      "before": ["d", "["],
      "after": ["$", "F", "[", "2", "x", "f", "]", "h", "2", "x"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"],
      "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
      "before": ["g", ")"],
      "after": ["$", "F", "(", "a"]
    },
    {
      "before": ["g", "}"],
      "after": ["$", "F", "{", "a"]
    },
    {
      "before": ["g", "]"],
      "after": ["$", "F", "[", "a"]
    },
    {
      "before": ["g", "("],
      "after": ["$", "F", "(", "a", "<cr>"]
    },
    {
      "before": ["g", "{"],
      "after": ["$", "F", "{", "a", "<cr>"]
    },
    {
      "before": ["g", "["],
      "after": ["$", "F", "[", "a", "<cr>"]
    },
    {
      "before": ["g", "m"],
      "after": ["o", "<esc>", "O", "<esc>"]
    },
    {
      "before": ["<space>", "j"],
      "commands": ["editor.action.insertLineAfter"],
      "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode == 'Normal'"
    },
    {
      "before": ["<space>", "k"],
      "commands": ["editor.action.insertLineBefore"],
      "when": "editorTextFocus && !editorReadonly && vim.active && !inDebugRepl && vim.mode != 'Normal'"
    },
    {
      "before": ["'"],
      "after": ["\\", "\\", "\\", "b", "d", "w"]
    },
    {
      "before": ["d", "w"],
      "after": ["d", "i", "w"]
    },
    {
      "before": ["c", "w"],
      "after": ["c", "i", "w"]
    },
    {
      "before": ["X"],
      "after": ["g", "g", "d", "G"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["%"],
      "commands": ["extension.matchitJumpItems"]
    },
    {
      "before": ["<space>", "b"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<space>", "p"],
      "commands": ["workbench.action.gotoSymbol"]
    },
    {
      "before": ["<space>", "r"],
      "commands": ["editor.action.rename"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<space>", "s"],
      "commands": ["workbench.action.files.save"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<space>", "1"],
      "commands": ["workbench.action.openSettingsJson"]
    },
    {
      "before": ["<space>", "x"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<space>", "S"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<space>", "c"],
      "commands": [":nohl"]
    },
    {
      "before": ["<S-l>"],
      "after": ["g", "_"]
    },
    {
      "before": ["<S-h>"],
      "after": ["^"]
      // "after": ["i", "<space>", "<esc>"]
    },
    {
      "before": ["<space>", "t"],
      "commands": [
        "workbench.action.terminal.toggleTerminal",
        "workbench.action.terminal.focus"
      ]
    },
    {
      "before": ["<space>", "e"],
      "commands": [
        "workbench.action.toggleSidebarVisibility",
        "revealInExplorer",
        "workbench.files.action.focusFilesExplorer"
      ]
    },
    {
      "before": ["<space>", "g"],
      "commands": [
        ":!alacritty -t 'LAZYGITCODE' --class 'lazygitcode' -e lazygit -p $(cat /tmp/lastdir) &"
      ]
    },
    {
      "before": ["<space>", "f"],
      "commands": [
        "workbench.action.quickOpen"
        // ":!alacritty -t 'FZFCODE' --  "vim.highlightedyank.duration": 1200,class 'fzfcode' -e fzfsearchforcode &"
      ]
    },
    {
      "before": ["<space>", "o"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<space>", "x"],
      "commands": [
        "workbench.action.closeAllEditors",
        "workbench.action.files.newUntitledFile"
      ]
    },
    {
      "before": ["<space>", "z"],
      "commands": [
        ":!alacritty -t 'VIFMCODE' --class 'vifmcode' -e vifm +only $(cat /tmp/lastdir) &"
      ]
    },
    {
      "before": ["<space>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<space>", "l"],
      "after": ["a", "<space>", "<esc>"]
    },
    {
      "before": ["<space>", "h"],
      "after": ["i", "<space>", "<esc>"]
    },
    {
      "before": ["<space>", "q"],
      "commands": ["workbench.action.showCommands"]
    }
  ],
