[
  {
    "context": "Workspace",
    "bindings": {
      "alt-c": "workspace::ToggleLeftDock",
      "cmd-space": "command_palette::Toggle"
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-[": "pane::SwapItemLeft",
      "cmd-]": "pane::SwapItemRight"
    }
  },
  {
    "context": "Workspace && !Terminal",
    "bindings": {
      "cmd-t": "workspace::NewFile"
    }
  },
  {
    "context": "ProjectPanel",
    "use_key_equivalents": true,
    "bindings": {
      "h": "project_panel::CollapseSelectedEntry",
      "l": "project_panel::Open",
      "cmd-l": ["workspace::SendKeystrokes", "enter control-w h alt-c"],
      "o": "project_panel::NewFile",
      "O": "project_panel::NewDirectory",
      "x": "project_panel::Cut",
      "y y": "project_panel::Copy",
      "p": "project_panel::Paste",
      "y P": "workspace::CopyPath",
      "y p": "workspace::CopyRelativePath",
      "r": "project_panel::Rename",
      "d d": ["project_panel::Trash", { "skip_prompt": true }],
      "/": "project_panel::NewSearchInDirectory",
      "escape": "menu::Cancel"
    }
  },
  {
    "bindings": {
      "cmd-h": null
    }
  },
  {
    "bindings": {
      "ctrl-cmd-shift-h": "zed::Hide"
    }
  },
  {
    "context": "((FileFinder || FileFinder > Picker > Editor) || FileFinder > Picker > menu)",
    "bindings": {
      "cmd-h": null
    }
  },
  {
    "context": "!(showing_code_actions || showing_completions)",
    "bindings": {
      "cmd-h": "pane::ActivatePreviousItem"
    }
  },
  {
    "context": "!(showing_code_actions || showing_completions)",
    "bindings": {
      "cmd-l": "pane::ActivateNextItem"
    }
  },
  {
    "context": "(Editor && vim_mode == normal && !(showing_code_actions || showing_completions))",
    "bindings": {
      "cmd-j": "editor::DuplicateLineDown"
    }
  },
  {
    "context": "(Editor && vim_mode == normal && !(showing_code_actions || showing_completions))",
    "bindings": {
      "cmd-k": "editor::DuplicateLineUp"
    }
  },
  {
    "context": "Picker > Editor",
    "bindings": {
      "cmd-k": "menu::SelectPrevious"
    }
  },
  {
    "context": "Picker > Editor",
    "bindings": {
      "cmd-l": "menu::Confirm"
    }
  },
  {
    "context": "(vim_mode == normal && !menu)",
    "bindings": {
      "space f": null
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "g r": "editor::FindAllReferences"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "g r r": null
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-j": "workspace::ToggleBottomDock"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-`": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-f": [
        "pane::DeploySearch",
        {
          "replace_enabled": true
        }
      ]
    }
  },
  {
    "bindings": {
      "f11": "debugger::StepInto"
    }
  },
  {
    "bindings": {
      "f10": "debugger::StepOver"
    }
  },
  {
    "context": "BreakpointList",
    "bindings": {
      "f9": "debugger::ToggleEnableBreakpoint"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "g u": "editor::UndoSelection"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-shift-u": "editor::UndoSelection"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "shift-l": "vim::EndOfLine"
    }
  },
  {
    "context": "Editor && !menu && VimControl && vim_mode == normal",
    "bindings": {
      "space b": "tab_switcher::ToggleAll",
      "space c a": "editor::ToggleCodeActions",
      "space e": "pane::RevealInProjectPanel",
      "space f": "file_finder::Toggle",
      "space j": ["workspace::SendKeystrokes", "o escape"],
      "space l": [
        "task::Spawn",
        { "task_name": "Open FZF", "reveal_target": "center" }
      ],
      "space k": ["workspace::SendKeystrokes", "O escape"],
      "space s": "workspace::Save",
      "space q q": ["pane::CloseActiveItem", { "close_pinned": true }],
      "space x": ["pane::CloseAllItems", { "close_pinned": true }],
      "space p": "outline::Toggle",
      "space /": [
        "editor::ToggleComments",
        {
          "advance_downwards": false
        }
      ]
    }
  },
  {
    "context": "(VimControl && !menu && !ProjectPanel)",
    "bindings": {
      "shift-h": "vim::FirstNonWhitespace"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "cmd-o": "pane::GoBack"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "cmd-i": "pane::GoForward"
    }
  },
  {
    "bindings": {
      "cmd-d": "editor::HalfPageDown"
    }
  },
  {
    "bindings": {
      "cmd-u": "editor::HalfPageUp"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "cmd-f": "vim::PageDown"
    }
  },
  {
    "context": "(VimControl && !menu)",
    "bindings": {
      "cmd-b": "vim::PageUp"
    }
  },
  {
    "context": "Editor && !menu && !VimWaiting && VimControl && vim_mode == normal && (vim_operator == none || vim_operator == n)",
    "bindings": {
      "g p d": "editor::GoToDefinitionSplit",
      "s i w": ["workspace::SendKeystrokes", "v i w p"],
      "s a w": ["workspace::SendKeystrokes", "v a w p"],
      "s i )": ["workspace::SendKeystrokes", "v i ) p"],
      "s a )": ["workspace::SendKeystrokes", "v a ) p"],
      "s i (": ["workspace::SendKeystrokes", "v i ( p"],
      "s a (": ["workspace::SendKeystrokes", "v a ( p"],
      "s i ]": ["workspace::SendKeystrokes", "v i ] p"],
      "s a ]": ["workspace::SendKeystrokes", "v a ] p"],
      "s i [": ["workspace::SendKeystrokes", "v i [ p"],
      "s a [": ["workspace::SendKeystrokes", "v a [ p"],
      "s i }": ["workspace::SendKeystrokes", "v i } p"],
      "s a }": ["workspace::SendKeystrokes", "v a } p"],
      "s i {": ["workspace::SendKeystrokes", "v i { p"],
      "s a {": ["workspace::SendKeystrokes", "v a { p"],
      "s i '": ["workspace::SendKeystrokes", "v i ' p"],
      "s a '": ["workspace::SendKeystrokes", "v a ' p"],
      "s i \"": ["workspace::SendKeystrokes", "v i \" p"],
      "s a \"": ["workspace::SendKeystrokes", "v a \" p"],
      "s s": ["workspace::SendKeystrokes", "shift-v p"],
      "g v": ["workspace::SendKeystrokes", "shift-v $ % $"],
      "g j f": [
        "task::Spawn",
        { "task_name": "Run Jest", "reveal_target": "center" }
      ]
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-w cmd-w": [
        "pane::CloseActiveItem",
        {
          "close_pinned": false
        }
      ]
    }
  },
  {
    "context": "Pane",
    "bindings": {
      "cmd-w": null
    }
  },
  {
    "context": "PromptLibrary",
    "bindings": {
      "cmd-w": null
    }
  },
  {
