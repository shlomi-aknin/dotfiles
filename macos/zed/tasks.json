[
  {
    "label": "Run Jest",
    "command": "./node_modules/.bin/jest",
    "args": ["--no-coverage", "$ZED_RELATIVE_FILE"],
    "env": { "foo": "bar" },
    "cwd": "$ZED_WORKTREE_ROOT",
    "use_new_terminal": false,
