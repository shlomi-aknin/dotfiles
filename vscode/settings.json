{
  "breadcrumbs.enabled": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "'SauceCodePro Nerd Font','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.minimap.enabled": false,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "explorer.compactFolders": false,
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.openEditors.visible": 30,
  "find-it-faster.general.showMaximizedTerminal": true,
  "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.cursorStylePerMode.visual": "underline",
  "vim.cursorStylePerMode.visualblock": "underline",
  "vim.cursorStylePerMode.visualline": "underline",
  "vim.easymotionIncSearchForegroundColor": "#f7wf38",
  "vim.easymotionMarkerForegroundColorOneChar": "#ffff00",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#ffff00",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#ffff00",
  "vim.highlightedyank.color": "#34a1eb",
  "vim.highlightedyank.duration": 1200,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "#000000",
  "vim.hlsearch": true,
  "vim.smartRelativeLine": true,
  "vim.searchHighlightColor": "#34a1eb90",
  "vim.useSystemClipboard": true,
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 2,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Dracula At Night",
  "workbench.list.smoothScrolling": true,
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "workbench.colorCustomizations": {
    "editorGroupHeader.tabsBackground": "#000000",
    "editorLineNumber.activeForeground": "#ffff00",
    "editor.lineHighlightBackground": "#235789",
    "editorBracketMatch.background": "#2176FF",
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["s"],
      "after": ["\\", "\\", "\\", "j"]
    },
    {
      "before": ["d", "w"],
      "after": ["d", "i", "w"],
    },
    {
      "before": ["c", "w"],
      "after": ["c", "i", "w"],
    },
    {
      "before": ["X"],
      "after": ["g", "g", "d", "G"],
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showDefinitionPreviewHover"],
    },
    {
      "before": ["%"],
      "commands": ["extension.matchitJumpItems"],
    },
    {
      "before": ["<space>", "l"],
      "commands": [":e!"]
    },
    {
      "before": ["<space>", "s"],
      "commands": [":w"]
    },
    {
      "before": ["<space>", "x"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<space>", "S"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<space>", "h"],
      "commands": [":nohl"]
    },
    {
      "before": ["<tab>", "l"],
      "after": ["a", "<space>", "<esc>"],
    },
    {
      "before": ["<tab>", "h"],
      "after": ["i", "<space>", "<esc>"],
    },
    {
      "before": ["<space>", "r"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<space>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal", "workbench.action.terminal.focus"]
    },
    {
      "before": ["<space>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility", "workbench.explorer.fileView.focus"]
    },
    {
      "before": ["<space>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<space>", "g"],
      "commands": [":!alacritty -e lazygit"]
    },
    {
      "before": ["<space>", "x"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<space>", "z"],
      "commands": [":!alacritty -e vifmrun $PWD"]
    },
    {
      "before": ["<space>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<C-o>"],
      "commands": ["workbench.action.files.openFolder"]
    },
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<space>", "/"],
      "commands": ["editor.action.commentLine"]
    },
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    },
  ],
  "tabnine.experimentalAutoImports": true,
  "php-cs-fixer.lastDownload": 1630388604811,
  "sync.autoDownload": true,
  "sync.forceUpload": true,
  "sync.autoUpload": true,
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "vim.easymotion": true,
  "sync.gist": "2e47474f8b6b139d6d1e2a491314795e",
  "settingsSync.ignoredExtensions": [
    "shan.code-settings-sync"
  ],
  "editor.tokenColorCustomizations": {
    "[Dracula At Night]": {
      "textMateRules": [
        {
          "scope": "comment",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#C69650",
          }
        },
        {
          "settings": {
            "foreground": "#ff79c6",
          },
          "scope": "support.class.component.svelte"
        }
      ],
    },
  }
}
